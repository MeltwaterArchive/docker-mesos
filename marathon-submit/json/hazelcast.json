{
  "id": "/myproduct/mysubsystem/hazelcast",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "meltwater/hazelcast:latest"
    }
  },
  "ports": [
    0
  ],
  "env": {
    "HZ_GROUP_NAME": "mygroup",
    "HZ_GROUP_PASSWORD": "secret"
  },
  "instances": 3,
  "cpus": 0.1,
  "mem": 500.0,
  "healthChecks": [
    {
      "protocol": "TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 15,
      "intervalSeconds": 10,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}