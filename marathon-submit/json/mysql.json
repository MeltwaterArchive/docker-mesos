{
  "id": "/myproduct/mysubsystem/mysql",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mysql:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 3306, "servicePort": 3306 }
      ]
    }
  },
  "env": {
    "MYSQL_ROOT_PASSWORD": "secret"
  },
  "instances": 1,
  "cpus": 0.1,
  "mem": 500.0,
  "healthChecks": [
    {
      "protocol": "TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 15,
      "intervalSeconds": 10,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 3
    }
  ]
}